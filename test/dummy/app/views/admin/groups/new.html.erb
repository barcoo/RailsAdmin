<div class="title-block"><h3 class="title">Register a new group</h3></div>
<%= render partial: 'kadmin/helpers/form_errors', locals: { model: @group_form } %>
<%= form_for [:admin, @group_form] do |form| %>
  <%= render partial: 'admin/groups/form', locals: { form: form, model: @group_form } %>
<% end %>

<% content_for(:stylesheets) do %>
  <%= stylesheet_link_tag "kadmin/select2", media: "all" %>
<% end %>

<% content_for(:javascripts) do %>
  <%= javascript_include_tag "kadmin/select2" %>
<% end %>
